{
    "manifest_version": 3,
    "name": "multiple_roles",
    "description": "chrome extension",
    "version": "0.0.1",
    "incognito":"split",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs",
        "nativeMessaging",
        "windows"
      ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://www.baidu.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/content.js"
            ]
        }
    ]
}